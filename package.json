    "storybook-generate": "sb-rn-get-stories --config-path .ondevice",
    "storybook:web": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "web": "webpack serve --mode=development --config webpack.config.ts",
    "web:build": "rm -rf dist/ && webpack --mode=production --config webpack.config.ts",
    "test": "jest --config=jest.config.js --json --outputFile=jest-test-results.json",
    "prepare": "husky",
    "build": "rm -rf lib && tsc && copyfiles -u 1 src/assets/**/* lib/src && copyfiles -u 1 'src/styles/**/*.css' lib/src && copyfiles package.json lib/ && node scripts/update-package-json.js"
